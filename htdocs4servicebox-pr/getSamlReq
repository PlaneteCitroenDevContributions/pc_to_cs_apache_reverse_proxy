else

    HTTP_REQ=$( curl \
		    -s \
		    -D - \
		    -A "${USERAGENT}" \
		    -X GET 'https://login-ra.fiat.com/accounts.webSdkBootstrap?apiKey='"${APIKEY}"'&pageURL='"${GIGYAURL}"'?SAMLRequest='"${SAMLREQUEST}"'&sdk=js_latest&sdkBuild=16506&format=json' \
		    | \
		    sed -e 's/\r$//'
	    )
